<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="MyToolbarStyle">
        <item name="android:minHeight">?actionBarSize</item>
        <item name="android:background">#007AFF</item>
        <item name="android:textColor">#ffffff</item>
        <item name="popupTheme">@style/ThemeOverlay.AppCompat.Light</item>

    </style>
    <style name="Theme.QINIU_SDK_ALL_IN_ONE" parent="Theme.MaterialComponents.Light.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#117AEC</item>
        <item name="colorPrimaryVariant">#007AFF</item>
        <item name="colorOnPrimary">#007AFF</item>
        <item name="toolbarStyle">@style/MyToolbarStyle</item>

        <!-- Secondary brand color. -->

        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.QINIU_SDK_ALL_IN_ONE_DARK" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#000000</item>
        <item name="colorPrimaryVariant">#000000</item>
        <item name="colorOnPrimary">#000000</item>
        <item name="toolbarStyle">@style/MyToolbarStyle</item>

        <!-- Secondary brand color. -->

        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

</resources>